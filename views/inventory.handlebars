<div class="jumbotron jumbotron-fluid">
  <h1 class="display-4"><a href="/">CarTech</a></h1>
  <p class="lead">Browse our inventory and see what you like</p>
  <hr class="my-4">
</div>

<div class="container">

  <h3>Cars:</h3>

  <ul id="cars-list" class="list-group">
 
      <div class="card-deck">   
        <div class="card-columns">
        {{#each car}}
        <div class="card" style="width:400px">
          <div class="card-block">
            <img src="{{this.photo}}" alt="car" style="width:100%">
            <h4 class=" card-title">{{this.year}} {{this.make}} {{this.model}}</h4>
            <p class="card-text">{{this.description}}</p>

            <a href="cars/{{this.id}}" class="btn btn-danger">See Car</a>
          </div>

        </div>
        {{/each}}
      </div>
    </div>
  </ul>


  </br>
  <a href="/">‚Üê Back To Home</a>
  </br>

</div>

